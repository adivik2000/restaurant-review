<% if @redirect_url.nil? %>
  $('#login_form').html("<%= escape_javascript(render(:partial => 'sessions/new.html.erb')) %>");
  $('#login_form').ajaxify({urlRef: false});
<% else %>
  $('#login_form').html("<%= escape_javascript(flash[:notice]) %>");
  <% if !@page_reload_off %>
    window.location.reload();
  <% end %>

  if (pageCallback != undefined) {
    pageCallback.call();
  }
<% end %>